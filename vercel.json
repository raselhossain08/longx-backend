{
    "name": "longx",
    "builds": [
      { "src": "app.js", "use": "@vercel/node" }
    ],
"routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "headers": [
                {
                  "key": "access-control-allow-origin",
                  "value": "*"
                }
            ]
        }
    ]
 
  }
  